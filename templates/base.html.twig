<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
		<meta http-equiv="X-UA-Compatible" content="IE=edge">
		<meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1" />
	    <meta name="viewport" content="width=device-width, initial-scale=1">
	    <meta name="description" content="JmLaZone91 Production">
	    <meta name="author" content="JmLaZone91">
	    <link rel="icon" type="image/png" href="{{ asset('img/youyou.png') }}" />
        <title>{% block title %}ASINT BG !{% endblock %}</title>
        {# Run `composer require symfony/webpack-encore-bundle`
           and uncomment the following Encore helpers to start using Symfony UX #}
        {% block stylesheets %}
            {#{{ encore_entry_link_tags('app') }}#}
			<link href="{{ asset('css/style.css') }}" rel="stylesheet" />
			<link href="{{ asset('bootstrap/css/bootstrap.min.css') }}" rel="stylesheet" />
			<link href="{{ asset('fontAwesome/css/all.min.css') }}" rel="stylesheet" />
			<link href="{{ asset('css/partenariat.css') }}" rel="stylesheet" />
        {% endblock %}

        {% block javascripts %}
            {#{{ encore_entry_script_tags('app') }}#}
            <script src="https://code.jquery.com/jquery-3.3.1.min.js" integrity="sha256-FgpCb/KJQlLNfOu91ta32o/NMZxltwRo8QtmkMRdAu8=" crossorigin="anonymous"></script>
            <script src="{{ asset('bootstrap/js/bootstrap.min.js') }}"></script>
            <script src="{{ asset('fontAwesome/js/all.min.js') }}"></script>
            <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js" integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM" crossorigin="anonymous"></script>
        {% endblock %}
    </head>
    <body>
        <nav class="navbar navbar-expand-lg navbar-light bg-light " style="padding-left:15px;padding-right:15px;">
    		  <a class="navbar-brand" href="{{ path('home') }}">
    		    <img src="{{ asset('img/logo.png') }}" width="60" height="50" class="d-inline-block align-top" alt="">
    		    <span class="asintFont asintColor asintNavLogo">ASINT</span>
    		  </a>
    		  <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
    		    <span class="navbar-toggler-icon"></span>
    		  </button>
    		  <div class="collapse navbar-collapse" id="navbarNav">
    		    <ul class="navbar-nav">
    		      <li class="nav-item active">
    		        <a class="nav-link asintColor" href="{{ path('sports') }}">Sports</a>
    		      </li>
    		      <li class="nav-item active">
    		        <a class="nav-link asintColor" href="{{ path('clubs') }}">Clubs</a>
    		      </li>
{#     		      <li class="nav-item">#}
{#     		        <a class="nav-link asintColor " href="{{ path('evenements') }}">Événements</a>#}
{#     		      </li>#}
    		      <li class="nav-item">
    		        <a class="nav-link asintColor" href="{{ path('partenaires') }}">Partenaires</a>
    		      </li>
    		      <li class="nav-item">
    		        <a class="nav-link asintColor" href="{{ path('equipe') }}">L'équipe</a>
    		      </li>
    		      <li class="nav-item">
    		        <a class="nav-link asintColor" href="{{ path('contact') }}">Contact</a>
    		      </li>
    		      <li class="nav-item">
    		        <a class="nav-link asintColor" href="{{ path('histoire') }}">Histoire</a>
    		      </li>
    		    </ul>
    		  </div>
    		    {% if is_granted('IS_AUTHENTICATED_FULLY') %}
	    		  	<div style="float:right">
	    		  		<ul class="navbar-nav">
	    		  			<li class="nav-item" style="margin-right:5px;">
	    		  				<a href="{{ path('admin91') }}">
	    		  					<i class="fas fa-user asintFont asintColor" style="font-size:25px"></i>
    		  					</a>
	    		  			</li>
							<li class="nav-item " style="margin-right:15px;">
								<a href="{{ path('admin91') }}">
		    			    		<h3 class="asintFont asintColor"> {{ app.user.username }}</h3>
		    			    	</a>
							</li>
							<li class="nav-item ">
								<a href="/logout"><i class="fas fa-sign-out-alt asintFont asintColor" style="font-size:25px"></i></a>
							</li>
	    		  		</ul>
		     		</div>
   		     	{% endif %}
    		</nav>
    	<div class="container">
    		<!-- = = = = = = = M E S S A G E S    F L A S H = = = = = = = = = = = = -->
			{% for message in app.flashes('success') %}
				<div class="alert alert-success">
					{{ message|raw }}
				</div>
			{% endfor %}
			{% for message in app.flashes('danger') %}
				<div class="alert alert-danger">
					{{ message|raw }}
				</div>
			{% endfor %}
    		<!-- = = = = = = = = = = = = = = = = = = = = = = = = = = = = = = = = = =-->
            {% block body %}
            
            {% endblock %}
